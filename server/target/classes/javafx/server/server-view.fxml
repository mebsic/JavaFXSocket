<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafx.server.ServerViewController">
   <children>
      <AnchorPane prefHeight="350.0" prefWidth="550.0">
         <children>
            <TextArea fx:id="console" editable="false" focusTraversable="false" prefHeight="350.0" prefWidth="550.0" style="-fx-background-color: #FFFFFF;">
               <effect>
                  <Blend opacity="0.0" />
               </effect>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </TextArea>
         </children>
      </AnchorPane>
   </children>
</VBox>
